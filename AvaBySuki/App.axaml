<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AvaBySuki.App"
             RequestedThemeVariant="Default"
             xmlns:sukiUi="clr-namespace:SukiUI;assembly=SukiUI"
             xmlns:templates="clr-namespace:ColorPicker.AvaloniaUI.Templates;assembly=ColorPicker.AvaloniaUI"
             xmlns:converters="clr-namespace:AvaBySuki.Converters">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <sukiUi:SukiTheme Locale="zh-CN" ThemeColor="Blue"  />
        <templates:PixiPerfectColorPickerTheme />
    </Application.Styles>
</Application>